{
  "name": "tax-hub",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "db:generate": "cd packages/database && pnpm prisma generate",
    "db:push": "cd packages/database && pnpm prisma db push",
    "db:studio": "cd packages/database && pnpm prisma studio",
    "setup": "pnpm install && pnpm db:generate"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}