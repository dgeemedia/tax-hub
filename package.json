{
  "name": "tax-hub",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "dev": "turbo run dev --filter=@taxhub/web",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "db:generate": "turbo run db:generate",
    "db:push": "cd packages/database && pnpm prisma db push",
    "db:studio": "cd packages/database && pnpm prisma studio",
    "setup": "pnpm install && turbo run db:generate"
  },
  "devDependencies": {
    "turbo": "^2.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@auth/core": "0.41.1",
    "@auth/prisma-adapter": "2.11.1"
  },
  "engines": {
    "node": "22.x",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "overrides": {
      "@auth/core": "0.41.1"
    }
  }
}